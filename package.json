{
  "name": "graphql-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for production-ready GraphQL server with authentication and protected auth routes",
  "main": "lib/app.js",
  "scripts": {
    "start": "node lib/app.js",
    "node": "nodemon lib/app.js",
    "build": "babel app --out-dir lib",
    "compile": "babel app --out-dir lib --watch --source-maps",
    "migrate": "./node_modules/.bin/sequelize db:migrate --migrations-path ./app/migrations"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "graphql-tag": "^2.10.3",
    "nodemon": "^1.18.10",
    "prettier": "^1.18.2",
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apollo-server-express": "^2.11.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-import": "^1.0.1",
    "graphql-tools-type-uuid": "^2.0.1",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^3.3.2",
    "yup": "^0.28.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnasledev/graphql-boilerplate.git"
  },
  "author": "Arnas Pecelis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arnasledev/graphql-boilerplate/issues"
  },
  "homepage": "https://github.com/arnasledev/graphql-boilerplate#readme"
}
